  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug: sls offline",
      "cwd": "${workspaceFolder}",
      "outFiles": ["${workspaceFolder}/**/*.js", "!**/node_modules/**"],
      "sourceMaps": true,
      "stopOnEntry": true,
      "console": "integratedTerminal",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "sls:offline:debug"],
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}